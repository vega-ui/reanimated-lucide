#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/slava/WebstormProjects/reanimated-lucide/node_modules/.pnpm/@svgr+cli@8.1.0_typescript@5.8.3/node_modules/@svgr/cli/bin/node_modules:/Users/slava/WebstormProjects/reanimated-lucide/node_modules/.pnpm/@svgr+cli@8.1.0_typescript@5.8.3/node_modules/@svgr/cli/node_modules:/Users/slava/WebstormProjects/reanimated-lucide/node_modules/.pnpm/@svgr+cli@8.1.0_typescript@5.8.3/node_modules/@svgr/node_modules:/Users/slava/WebstormProjects/reanimated-lucide/node_modules/.pnpm/@svgr+cli@8.1.0_typescript@5.8.3/node_modules:/Users/slava/WebstormProjects/reanimated-lucide/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/slava/WebstormProjects/reanimated-lucide/node_modules/.pnpm/@svgr+cli@8.1.0_typescript@5.8.3/node_modules/@svgr/cli/bin/node_modules:/Users/slava/WebstormProjects/reanimated-lucide/node_modules/.pnpm/@svgr+cli@8.1.0_typescript@5.8.3/node_modules/@svgr/cli/node_modules:/Users/slava/WebstormProjects/reanimated-lucide/node_modules/.pnpm/@svgr+cli@8.1.0_typescript@5.8.3/node_modules/@svgr/node_modules:/Users/slava/WebstormProjects/reanimated-lucide/node_modules/.pnpm/@svgr+cli@8.1.0_typescript@5.8.3/node_modules:/Users/slava/WebstormProjects/reanimated-lucide/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/svgr" "$@"
else
  exec node  "$basedir/../../bin/svgr" "$@"
fi
